                "failMsg": {
                  "type": "string",
                  "maxLength": 300,
                  "description": "Error message if applicable"
                }
              },
              "required": [
                "status"
              ]
            }
          }
        },
        "description": "get the transfer status for small amount verification",
        "tags": [
          "Member"
        ]
      }
    },
